{
  "name": "cosequin_canada",
  "version": "1.0.0",
  "description": "This is just to run ejs templates on my Cosequin Canada stack",
  "main": "app.js",
  "scripts": {
    "build": "npx npm-run-all build:* --npm-path npm",
    "build:home": "npx ejs ./src/index.ejs -o ./build/index.html -f ./src/data/index.json;",
    "build:dog-products": "npx ejs ./src/products.ejs -o ./build/dog_products.html -f ./src/data/dog-product.json;",
    "build:cat-products": "npx ejs ./src/products.ejs -o ./build/cat_products.html -f ./src/data/cat-products.json;",
    "build:dog-chews": "npx ejs ./src/product-page.ejs -o ./build/cosequin-msm-omega3.html -f ./src/data/dog-chews.json;",
    "build:dog-chewables": "npx ejs ./src/product-page.ejs -o ./build/cosequin-chewable-tablets.html -f ./src/data/dog-chewable-tablets.json;",
    "build:cat-extra-strength": "npx ejs ./src/product-page.ejs -o ./build/cosequin-cat-extra-strength.html -f ./src/data/cat-capsules-extra-strength.json;",
    "build:cat": "npx ejs ./src/product-page.ejs -o ./build/cosequin-cat.html -f ./src/data/cat-capsules.json;"
  },
  "author": "David Lothers",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.8"
  },
  "devDependencies": {
    "ejs-lint": "^2.0.0",
    "npm-run-all": "^4.1.5"
  }
}
