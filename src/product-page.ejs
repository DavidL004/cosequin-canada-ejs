    <%- include('src/partials/horizontal-rule', {src: 'https://s3.amazonaws.com/static.cosequin.com/ca/nmx_icons/icon_book.svg'}); %>
    <section class="product-details">
        <h3>
            Product Details
        </h3>
        <div>
            <div class="expandable closed">
                <div>
                   <h4>
                        Product Description
                   </h4>
