<div class="product<%=(animalType==defaultAnimal) ? "" : " hidden"%>" data-animal="<%=animalType%>">
    <img src="<%=`${ImageAWSurl}${urls.imgURL}`%>" alt="">
    <p><em><%= type %></em></p>
    <h4><%- name %></h4>
    <div class="expandable closed">
        <div>
            <p>Buy Now</p>
            <img class="open_close_icon" src="<%=`${GoogleIconsAWSurl}expand_more_white.svg`%>" data-image-open="<%=`${GoogleIconsAWSurl}expand_less_white.svg`%>" data-image-close="<%=`${GoogleIconsAWSurl}expand_more_white.svg`%>" alt="dropdown">
        </div>
        <div>
           <ul>
            <li><a href="<%= urls.amazonURL %>">Buy Now on Amazon</a></li>
            <li><a href="<%= urls.costcoURL %>">Buy Now on Costco</a></li>
            <li><a href="<%= urls.petSmartURL %>">Buy Now on PetSmart</a></li>
           </ul>
        </div>
    </div>
    <a href="<%= urls.productPageURL %>">Learn More</a>
</div>